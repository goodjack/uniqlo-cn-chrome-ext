{
  "manifest_version": 2,
  "name": "Uniqlo CN Extension",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "http://www.uniqlo.cn/*"
      ],
      "js": ["jquery-2.2.4.min.js", "kotlin.js", "kotlin.meta.js", "common.js", "common.meta.js", "content.js", "content.meta.js"]
    }
  ],
  "background": {
    "scripts": ["jquery-2.2.4.min.js", "kotlin.js", "kotlin.meta.js", "common.js", "common.meta.js", "background.js", "background.meta.js"]
  },
  "browser_action": {
    "default_icon": "UNIQLO_logo.png"
  }
}